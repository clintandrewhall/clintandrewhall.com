    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.2",
    "rellax": "^1.12.1",
    "responsive-loader": "^2.3.0",
    "swiper": "^6.5.7",
