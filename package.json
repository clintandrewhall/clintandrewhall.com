    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yaml-front-matter": "^4.1.1"
  },
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier:fix": "pretty-quick",
    "prettier:staged": "pretty-quick --staged",
    "stylelint": "stylelint src",
    "stylelint:fix": "stylelint src --fix",
    "format": "yarn prettier:fix && yarn eslint:fix && yarn stylelint:fix",
    "lint": "yarn eslint && yarn stylelint",
    "generate:resume": "hackmyresume build ./src/content/resume.json TO ./public/resume/clintandrewhall.all -t ./src/resume && rm ./public/resume/clintandrewhall.p*.html && rm ./public/resume/*.css",
    "generate:github": "babel-node --extensions .ts --presets @babel/preset-env,@babel/preset-typescript ./scripts/github",
    "generate:medium": "babel-node --extensions .ts --presets @babel/preset-env,@babel/preset-typescript ./scripts/medium",
    "generate:sitemap": "react-snap-sitemap --base-url='https://www.clintandrewhall.com/'",
    "start:resume": "nodemon --exec \"clear && yarn run generate:resume\" -e json,hbs,css,html --watch ./src/resume --watch ./src/content",
    "start:site": "concurrently \"yarn react-scripts start\" \"node scripts/dev\"",
    "start": "node scripts/server",
    "test": "react-scripts test",
    "build": "react-scripts build && yarn generate:github && yarn generate:medium",
    "postbuild": "node scripts/snap && yarn generate:sitemap",
    "eject": "react-scripts eject"
  },
  "reactSnap": {
    "fixWebpackChunksIssue": "CRA2",
    "cacheAjaxRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true
  },
  "engines": {
    "yarn": ">=1.9.4",
    "node": "14.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
