    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.2",
    "react-slide-toggle": "^0.3.5",
    "rellax": "^1.12.1",
    "responsive-loader": "^2.3.0",
