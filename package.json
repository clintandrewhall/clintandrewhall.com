  "private": true,
  "license": "None; private",
  "homepage": "https://www.clintandrewhall.com",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-es2015": "^6.24.1",
    "canvas": "^1.6.11",
    "css-modules-flow-types-cli": "^1.1.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.80.0",
    "hackmyresume": "^1.9.0-beta",
    "handlebars-helpers": "^0.10.0",
    "husky": "^0.14.3",
    "jimp": "^0.4.0",
    "lint-staged": "^7.2.2",
    "markdown-image-loader": "^1.0.5",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "react-app-rewire-css-modules": "codebandits/react-app-rewire-css-modules",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "^1.1.5",
    "react-snapshot-sitemap": "^0.1.0",
    "responsive-loader": "^1.1.0",
    "sass-loader": "^6.0.7",
    "webpack": "3.12.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fbemitter": "^2.1.1",
    "flickity": "^2.1.2",
    "github-markdown-css": "^2.10.0",
    "helper-markdown": "^1.0.0",
    "markdown-with-front-matter-loader": "^0.1.0",
    "masonry-layout": "^4.2.2",
    "moment": "^2.22.2",
    "multi-loader": "^0.1.0",
    "numeral": "^2.0.6",
    "photoswipe": "^4.1.2",
    "ramda": "^0.25.0",
    "react": "^16.5.0",
    "react-dom": "^16.4.2",
    "react-flickity-component": "^3.0.3",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.6.0",
    "react-router-dom": "^4.3.1",
    "react-slide-toggle": "^0.1.13",
    "react-snapshot": "^1.3.0",
    "react-waypoint": "^8.0.3",
    "rellax": "^1.6.2",
    "request": "^2.88.0",
    "smoothscroll": "^0.4.0"
  },
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,json,md}'",
    "format:changed": "pretty-quick",
    "format:staged": "pretty-quick --staged",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint **/*.js --quiet",
    "lint-fix": "eslint --fix",
    "develop:resume": "nodemon --exec \"clear && yarn run generate:resume\" -e json,hbs,css,html -w ./src/resume -w ./src/_content/resume",
    "develop:site": "react-app-rewired start",
    "generate:cssflowtypes": "css-modules-flow-types src/components",
    "generate:resume": "hackmyresume build ./src/_content/resume.json TO ./public/resume/clintandrewhall.all -t ./src/resume",
    "generate": "yarn generate:cssflowtypes && yarn generate:resume",
    "build:github": "babel-node --presets babel-preset-env --plugins babel-plugin-transform-flow-strip-types -- ./data/github",
    "build:medium": "babel-node --presets babel-preset-env --plugins babel-plugin-transform-flow-strip-types -- ./data/medium",
    "build:site": "react-app-rewired build",
    "build:sitemap": "react-snapshot-sitemap --output-domain https://clintandrewhall.com",
    "build:snapshots": "react-snapshot",
    "build": "yarn build:medium && yarn build:github && yarn build:site && yarn build:snapshots && yarn build:sitemap",
    "eject": "react-scripts eject",
    "start": "node server",
    "test": "react-app-rewired test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "generate && pretty-quick --staged"
    }
  },
  "engines": {
    "yarn": ">=1.9.4",
    "node": ">=10.9.0"
  }
}
