  "scripts": {
    "build:github": "babel-node --presets babel-preset-env --plugins babel-plugin-transform-flow-strip-types -- ./data/github",
    "build:medium": "babel-node --presets babel-preset-env --plugins babel-plugin-transform-flow-strip-types -- ./data/medium",
    "build:css": "css-modules-flow-types src",
    "build:resume": "hackmyresume build ./src/_content/resume.json TO ./build/resume/index.all -t ./src/resume",
    "build:site": "react-scripts build",
    "build": "yarn run build:site && yarn run build:css && yarn run build:medium && yarn run build:github && yarn run build:resume",
    "develop": "react-scripts start",
    "eject": "react-scripts eject",
    "flow": "flow",
    "start": "serve -s build",
    "test": "react-scripts test --env=jsdom"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
