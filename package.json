    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yaml-front-matter": "^4.1.1"
  },
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "eslint:prettier": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prettier:fix": "pretty-quick",
    "prettier:staged": "pretty-quick --staged",
    "stylelint": "stylelint src",
    "stylelint:fix": "stylelint src --fix",
    "format": "yarn prettier:fix && yarn eslint:fix && yarn stylelint:fix",
    "lint": "yarn eslint && yarn stylelint",
    "generate:resume": "hackmyresume build ./src/_content/resume.json TO ./public/resume/clintandrewhall.all -t ./src/resume",
    "generate:github": "babel-node --extensions .ts --presets @babel/preset-env,@babel/preset-typescript ./data/github",
    "develop:resume": "nodemon --exec \"clear && yarn run generate:resume\" -e json,hbs,css,html -w ./src/resume -w ./src/_content/resume",
    "develop:site": "react-scripts start",
    "start": "node server",
    "test": "react-scripts test",
    "build": "yarn generate:github && react-scripts build",
    "postbuild": "react-snap && react-snap-sitemap --base-url='https://www.clintandrewhall.com/'",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn generate:resume && yarn format && git add ."
    }
  },
  "reactSnap": {
    "cacheAjaxRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "engines": {
    "yarn": ">=1.9.4",
    "node": "14.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}