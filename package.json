    "stylelint-config-standard": "^18.2.0",
    "webpack": "3.12.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fbemitter": "^2.1.1",
    "flickity": "^2.1.2",
    "github-markdown-css": "^2.10.0",
    "helper-markdown": "^1.0.0",
    "markdown-with-front-matter-loader": "^0.1.0",
    "masonry-layout": "^4.2.2",
    "moment": "^2.22.2",
