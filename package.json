    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yaml-front-matter": "^4.1.1"
  },
  "scripts": {
    "lint:es": "eslint .",
    "lint:es:fix": "eslint . --fix",
    "lint:prettier": "pretty-quick --staged",
    "lint:prettier:fix": "pretty-quick",
    "lint:style": "stylelint src",
    "lint:style:fix": "stylelint src --fix",
    "lint": "yarn lint:prettier &&yarn lint:es && yarn lint:style",
    "lint:fix": "yarn lint:prettier:fix && yarn lint:es:fix && yarn lint:style:fix",
    "generate:resume": "",
    "generate:github": "babel-node --extensions .ts --presets @babel/preset-env,@babel/preset-typescript ./scripts/github",
    "generate:medium": "babel-node --extensions .ts --presets @babel/preset-env,@babel/preset-typescript ./scripts/medium",
    "generate:sitemap": "react-snap-sitemap --base-url='https://www.clintandrewhall.com/'",
    "start:resume": "",
    "start": "concurrently \"yarn react-scripts start\" \"node scripts/dev\"",
    "test": "react-scripts test",
    "build": "react-scripts build && yarn generate:github && yarn generate:medium",
    "postbuild": "node scripts/snap && yarn generate:sitemap",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -r https://github.com/clintandrewhall/clintandrewhall.github.io",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "reactSnap": {
    "fixWebpackChunksIssue": "CRA2",
    "cacheAjaxRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true
  },
  "engines": {
    "yarn": ">=1.9.4",
    "node": "16.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "**/*.ts?(x)": "tsc-files --noEmit types/github.d.ts types/global.d.ts types/medium.d.ts types/resume.d.ts"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
